Drop Table Documents;
create table Documents (id INTEGER, content TEXT, originalFile TEXT, date TEXT, user INTEGER, inZipFile TEXT, sizeOfOriginalFile INTEGER);
Drop Table Bons;
create table Bons (belongsToDocument INTEGER, sum REAL);
Drop Table AllowedUsers;
create Table AllowedUsers(id INTEGER, name TEXT);
Drop Table Tags;
create Table Tags(belongsToDOcument, Tag TEXT);
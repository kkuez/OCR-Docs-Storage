Drop Table Documents;
create table Documents (id INTEGER, content TEXT, originalFile TEXT, date TEXT, user INTEGER, inZipFile TEXT, sizeOfOriginalFile INTEGER);
Drop Table Bons;
create table Bons (belongsToDocument INTEGER, sum REAL);
Drop Table AllowedUsers;
create Table AllowedUsers(id INTEGER, name TEXT, chatId INTEGER, hasXORKey INTEGER);
Drop Table Tags;
create Table Tags(belongsToDocument INTEGER, Tag TEXT);
Drop Table ShoppingList;
create Table ShoppingList(item TEXT);
Drop Table StandardList;
create Table StandardList(item TEXT);
Drop Table CalendarTasks;
create Table CalendarTasks(year INTEGER, month INTEGER, day INTEGER, hour INTEGER, minute INTEGER, name TEXT, user TEXT, taskType TEXT, strategyType TEXT, eID TEXT);
Drop Table Memos;
create Table Memos(item TEXT, user INTEGER);
Drop Table QRItems;
create Table QRItems(itemNumber INTEGER, itemMapped TEXT);

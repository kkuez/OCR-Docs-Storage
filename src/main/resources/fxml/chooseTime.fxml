<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Controller.ChooseTimeController">
   <children>
      <RadioButton fx:id="lastMonthRadio" layoutX="45.0" layoutY="38.0" mnemonicParsing="false" onAction="#checklastMonthRadio" text="Last Month" />
      <RadioButton fx:id="lastSixMonthRadio" layoutX="45.0" layoutY="75.0" mnemonicParsing="false" onAction="#checklastSixMonthRadio" text="Last 6 Month" />
      <RadioButton fx:id="lastYearRadio" layoutX="45.0" layoutY="113.0" mnemonicParsing="false" onAction="#checklastYearRadio" text="Last Year" />
      <RadioButton fx:id="specificTimeRadio" layoutX="45.0" layoutY="149.0" mnemonicParsing="false" onAction="#checkspecificTimeRadio" text="Specific Time" />
      <TextField fx:id="specificTimeOneTextField" layoutX="164.0" layoutY="145.0" promptText="f.e. 09/2019" />
      <TextField fx:id="specificTimeTwoTextField" layoutX="349.0" layoutY="145.0" promptText="f.e. 09/2019 or nothing" />
      <Label layoutX="326.0" layoutY="149.0" text="-" />
      <Button fx:id="submitButton" layoutX="126.0" layoutY="245.0" mnemonicParsing="false" onAction="#submit" prefHeight="25.0" prefWidth="343.0" text="Submit" />
   </children>
</AnchorPane>
